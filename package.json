{
  "name": "personal-site",
  "version": "1.0.0",
  "description": "My personal site, built with Nuxt, Tailwind, and Netlify.",
  "author": "Matt Waler",
  "private": true,
  "scripts": {
    "analyze": "nuxt build -c config/nuxt.js -a",
    "dev": "nuxt -c config/nuxt.js",
    "functions": "netlify-lambda serve functions",
    "gen": "netlify-lambda build functions && nuxt generate -c config/nuxt.js",
    "start": "nuxt start -c config/nuxt.js",
    "test": "jest -c=config/jest.config.js"
  },
  "dependencies": {
    "@nuxtjs/dotenv": "^1.3.0",
    "@nuxtjs/pwa": "^3.0.0-beta.14",
    "axios": "^0.18.0",
    "cross-env": "^5.2.0",
    "nuxt": "^2.4.0",
    "nuxt-svg-loader": "^1.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@fullhuman/postcss-purgecss": "^1.1.0",
    "@vue/cli-plugin-babel": "^3.4.1",
    "@vue/test-utils": "^1.0.0-beta.27",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "eslint": "^5.14.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^22.3.2",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-vue": "^5.2.2",
    "jest": "^23.6.0",
    "netlify-lambda": "^1.4.5",
    "nodemon": "^1.18.9",
    "postcss-clean": "^1.1.0",
    "postcss-easy-import": "^3.0.0",
    "postcss-easy-media-query": "^1.0.0",
    "postcss-nested": "^4.1.2",
    "tailwindcss": "^1.0.0-beta.3",
    "vue": "^2.6.7",
    "vue-jest": "^3.0.2",
    "vue-loader": "^15.7.0",
    "vue-template-compiler": "^2.6.7",
    "webpack": "^4.29.5"
  }
}
